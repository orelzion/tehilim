    <eat-comment />

    <style name="HSActionBarCompat">

        <!-- Action Bar background -->
        <item name="android:background">#222222</item>
    </style>

    <style name="HSActionBarCompatItemBase">
        <item name="android:scaleType">center</item>
        <item name="android:background">?attr/hs__selectableItemBackground</item>
    </style>

    <style name="HSActionBarCompatProgressIndicator" parent="android:style/Widget.ProgressBar.Large">
        <item name="android:indeterminate">true</item>
    </style>

    <style name="HSActionBarCompatTitle" parent="HSActionBarCompatTitleBase">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="HSActionBarCompatTitleBase">
        <item name="android:id">@id/hs__actionbar_compat_title</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:paddingRight">6dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
    </style>

    <style name="HSActionBarThemedContext">

        <!-- Action Bar text color primary and secondary colors -->
        <item name="android:textColorPrimary">@color/hs__actionBarTextColorPrimary</item>
        <item name="android:textColorSecondary">@color/hs__actionBarTextColorSecondary</item>
    </style>

    <style name="HSEditTextAppTheme" parent="android:Widget.EditText">
        <item name="android:background">@drawable/hs__edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Theme.Helpshift" parent="@style/Theme.Helpshift.Base">
        <item name="android:windowNoTitle">false</item>
        <item name="android:windowTitleSize">@dimen/hs__actionbar_compat_height</item>
        <item name="android:windowTitleBackgroundStyle">@style/HSActionBarCompat</item>
        <item name="android:windowContentOverlay">@drawable/hs__actionbar_shadow</item>
        <item name="android:editTextStyle">@style/HSEditTextAppTheme</item>

        <!-- Action Bar Menu Item Selectable item background -->
        <item name="hs__selectableItemBackground">@drawable/hs__actionbar_compat_item</item>
        <item name="hs__actionbarCompatTitleStyle">@style/HSActionBarCompatTitle</item>
        <item name="hs__actionbarCompatItemBaseStyle">@style/HSActionBarCompatItemBase</item>
        <item name="hs__actionBarTabTextStyle">@style/Widget.Helpshift.Base.ActionBar.TabText</item>
        <item name="hs__actionbarCompatProgressIndicatorStyle">@style/HSActionBarCompatProgressIndicator</item>
    </style>
